<h1>Cadastrar uma cozinha</h1>
<%= render partial: "shared/errors", locals: {object: @cuisine} %>

<%= form_for @cuisine do |f| %>
  <div>
    <%= f.label :name, 'Nome' %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.submit 'Enviar' %>
  </div>
<% end %>
<p>
  <%= link_to "Voltar", root_path %>
</p>
